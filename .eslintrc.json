{
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "amd": true
    },
    "extends": ["eslint:recommended", "google"],
    "rules": {
        "no-console": 0,
        "max-len": ["error", { "code": 100, "ignoreComments": true }],
        "quote-props": ["error", "consistent-as-needed"]
    }
}